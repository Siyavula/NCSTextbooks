1.00 0 -16 0 resistor
1.00 315 0 16 arrow
1  1.00 -16 0 -48 -32 2 polygon
endgate
} def

/resistor2 {
% -64 -16 128 32 bbox
begingate
1  1.00 -64 0 -48 0 2 polygon
0  1.00 -48 16 -48 -16 48 -16 48 16 4 polygon
1  1.00 48 0 64 0 2 polygon
endgate
} def

